<%= form_tag searchedrooms_path, method: :get do %>
  <div class="checkinout">
    <%= date_field :start_at, params[:start_at], required: false, id: "range_start", class: "calendar_form" %>
    <%= date_field :end_at, params[:end_at], required: false, id: "range_end", type: :text, class: "calendar_form flatpickr-input" %>
  </div>
  <div class="buttonbookcalendar">
    <%= submit_tag "Looking for rooms", class: "button-access-calendar" %>
  </div>
<% end %>