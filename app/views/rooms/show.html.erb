<div class="banner-show mb-4">
  <span class="name-show"><%= @room.category.name %></span>
  <div class="price-grid">
    <span class="price-show"><%= humanized_money_with_symbol(@room.category.price)%><span class="size-price-banner"><em><%= t('page.rooms.night') %></em></span></span>
  </div>
</div>

<div class="container-white">
  <div class="photo-room-calendar">
    <div class="photo-room">
      <div id="carouselExampleIndicators" class="carousel slide img-caroussel" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active">
            <%= image_tag 'rooms/' + @room.photo, class: 'd-block w-100 height-carroussel' %>
          </div>
          <div class="carousel-item">
            <%= image_tag 'rooms/' + @room.phototwo, class: 'd-block w-100 height-carroussel' %>
          </div>
          <div class="carousel-item">
            <%= image_tag 'rooms/' + @room.photothree, class: 'd-block w-100 height-carroussel' %>
          </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>


          <div class="services-show my-4">
            <div class="balcony-show">
              <% if @room.category.balcony %>
                <div>
                  <span> <img id="icone-css-show"src="data:image/svg+xml;base64,"/> </span>
                </div>
                <div>
                  <span><%= t('page.rooms.balcony') %></span>
                </div>
              <% elsif @room.category.corner_table %>
                <div>
                  <span> <img id="icone-css-show" src="data:image/svg+xml;base64," /></span>
                </div>
                <div>
                  <span><%= t('page.rooms.corner-table')%></span>
                </div>
              <% end %>
            </div>
            <div class="bed-size">
              <div>
                <span><img id="icone-css-show" src="data:image/svg+xml;base64," /></span>
              </div>
              <div>
                <span> <%= @room.category.bed_size %></span>
              </div>
            </div>
