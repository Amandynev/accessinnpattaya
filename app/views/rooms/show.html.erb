        <span class="size-number-show "><%= @room.category.room_size%></span> sqm</span>
        </div>

        <div class="balcony">
          <% if @room.category.balcony %>
            <span> <img id="icone-css-show"src="data:image/svg+xml;base64,"/>Room with balcony</span>
          <span><% elsif @room.category.corner_table %></span>
            <span> <img id="icone-css-show" src="data:image/svg+xml;base64," />Corner table</span>
          <% end %>
        </div>
      </div>
      <div>
        <hr>
        <br>
      </div>
      <div class="description-show">
        <p><%=@room.category.description%></p>
      </div>
      <br>
      <hr>
      <br>
    </div>

    <div class="calendar-booking">
      <%= render '/bookings/form', booking: @booking %>
    </div>
  </div>
</div>

<div class="container-white">
  <h2 class="title-room-show"> Room services </h2>

  <div class="services-grid">
    <% Room::SERVICES.each do |service| %>
      <div class="service-item">
        <%= service %>
      </div>
    <% end %>
  </div>

</div>

<% if @modal_cancel %>
  <span id="booking-canceled"></span>
<% end %>
<% if params[:param] %>
  <span id="booking-success"></span>
<% end %>
