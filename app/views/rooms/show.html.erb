          <h2 class="title-room-show"> <%= t('page.rooms.around')%>  </h2>
          <div class="hotel-facilities">
            <div class="container-card-facilities">
                <%= link_to restaurant_path, class:"link-without-style" do %>
                  <div class="card-category-facilities px-1" data-aos="flip-up"data-aos-duration="1000" style="background-image: url(<%= asset_path 'restaurant/restaurant2.jpg' %>)">
                    <p class="card-title-square-show">RESTAURANT</p>
                  </div>
                <%end%>
            </div>

            <div class="container-card-facilities">
                <%= link_to restaurant_path, class:"link-without-style" do %>
                  <div class="card-category-facilities px-1" data-aos="flip-up"data-aos-duration="1000" style="background-image: url(<%= asset_path 'restaurant/pooltable.jpg' %>)">
                    <p class="card-title-square-show">BAR</p>
                  </div>
                <%end%>
            </div>

            <div class="container-card-facilities">
                <%= link_to event_path, class:"link-without-style" do %>
                  <div class="card-category-facilities px-1" data-aos="flip-up"data-aos-duration="1000" style="background-image: url(<%= asset_path 'event/birthdaywhite.jpg' %>)">
                    <p class="card-title-square-show"><%= t('page.home.restaurant.title')%></p>
                  </div>
                <%end%>
            </div>
          </div>
  </div>
  <!------------------------- column calendar et card product -------------------------------->

    <div class="column-calendar">
      <div class="calendar-booking">
        <%= render '/bookings/form', booking: @booking %>
      </div>
      <div class="display-flex-similar-room">
        <h3 class="title-similar-room mt-5"><%= t('page.rooms.similar')%> </h3>
        <p class="line">_______</p>
        <% @categories.map { |cat| cat.rooms.first }.each do |room| %>
          <div class="card-product">
            <%= image_tag "rooms/"+ room.photo, class: 'photo-card-product' %>
            <div class="card-product-infos">
              <p class="title-card-product"><%= room.category.name.upcase %></p>
              <p class="price-card-product"><%= t('page.home.from')%> <%= humanized_money_with_symbol(room.category.price)%><%= t('page.rooms.night')%></p>
              <div>
                <%= link_to t('calendar.book-calendar'), room_path(room), class: "button-access-book-small link-without-style"%>
              </div>
            </div>
          </div>
        <%end%>
      </div>
    </div>

  </div>
</div>


<% if params[:param] == "nok" %>
  <span id="booking-canceled"></span>
<% end %>
<% if params[:param] == "ok" %>
  <span id="booking-success"></span>
<% end %>
